javascript:
  $(function() {
  $('#datetimepicker-start').datetimepicker();
  $('#datetimepicker-end').datetimepicker();
  $("#datetimepicker-start").on("dp.change",function (end) {
    $('#datetimepicker-end').data("DateTimePicker").minDate(end.date);
  });
  $("#datetimepicker-end").on("dp.change",function (end) {
    $('#datetimepicker-start').data("DateTimePicker").maxDate(end.date);
  });
  });

javascript:
  $(function() {
    var options = {
      map: "#map-canvas",
      types: ["geocode", "establishment"],
    };
    map = $("#geocomplete").geocomplete(options)
    map.bind("geocode:result", function(event, result) {
      $.log = ("Result: " + result.formatted_address);
    });
    $("#trigger-map").click(function() {
      google.maps.event.trigger(map, 'resize');
    });
  });

= javascript_include_tag 'application'
